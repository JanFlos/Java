package dml;

public class TableColumn {

	String _name;
	int	  _type;
	
	public TableColumn(String name, int type, int displaySize) {
		_name = name.toLowerCase();
		_type = type;
		_displaySize = displaySize;
	}

	public String getName() {
		return _name;
	}

	public int getType() {
		return _type;
	}


	
	
}
